<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%
	String webpath = request.getContextPath();
%>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta http-equiv="expires" content="0">
<meta http-equiv="Pragma" content="no-cache">
<meta http-equiv="Cache-Control" content="no-cache" />

<!-- 引入框架样式 -->
<link rel="stylesheet" type="text/css"
	href="<%=webpath%>/transaction/main/Assets/css/reset.css" />
<link rel="stylesheet" type="text/css"
	href="<%=webpath%>/transaction/main/Assets/css/common.css" />
<link rel="stylesheet" type="text/css"
	href="<%=webpath%>/transaction/main/Assets/css/thems.css">

<!-- 引入jquery -->
<script type="text/javascript"
	src="<%=webpath%>/common/js/jquery-easyui-1.5.5.1/jquery.min.js"></script>
<!-- 引入easyui js -->
<script type="text/javascript"
	src="<%=webpath%>/common/js/jquery-easyui-1.5.5.1/jquery.easyui.min.js"></script>
<!-- 引入国际化js -->
<script type="text/javascript"
	src="<%=webpath%>/common/js/jquery-easyui-1.5.5.1/locale/easyui-lang-zh_CN.js"></script>
<!-- 引入esayui样式 -->
<link rel="stylesheet" type="text/css"
	href="<%=webpath%>/common/js/jquery-easyui-1.5.5.1/themes/bootstrap/easyui.css">
<link rel="stylesheet" type="text/css"
	href="<%=webpath%>/common/js/jquery-easyui-1.5.5.1/themes/icon.css">


<!-- 引入公共js -->
<script type="text/javascript" src="<%=webpath%>/common/js/panelCss.js"></script>
<!-- 引入继承EASYUI校验js -->
<script type="text/javascript" src="<%=webpath%>/common/extendEasyUI.js"></script>

<script src="https://img.hcharts.cn/highcharts/highcharts.js"></script>
<script src="https://img.hcharts.cn/highcharts/modules/exporting.js"></script>
<script src="https://img.hcharts.cn/highcharts/modules/oldie.js"></script>
<script src="https://img.hcharts.cn/highcharts-plugins/highcharts-zh_CN.js"></script>

<!-- 引入浏览器处理js -->
<script type="text/javascript" src="<%=webpath%>/common/js/browser.js"></script>
<script type="text/javascript">
	$(function(){
		$("#clearQuery").click(function(){
			$(".easyui-datebox").datebox("setValue",""); 
			$(".easyui-combobox").combobox("setValue",""); 
		});
	});
</script>
